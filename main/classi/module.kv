# ----- Label Module per library ------------------------------------------------------------------------------------- #
<ModuleLabel>:
    size_hint: None, None
    size: 100, 100
    font_size: 14
    canvas.before:
        Color:
            rgba: GV.RGBA_BG_DARK
        Rectangle:
            size: self.size
            pos: self.pos
        Color:
            rgba: GV.RGBA_MODULE
        Line:
            rectangle: self.x, self.y, self.width, self.height
            width: 1.2

# ----- Classe grafica pulsanti di creazione Module ----- #
<EditModuleBtn>:
    size: 24, 24
    size_hint: None, None
    background_down: ''
    background_normal: ''
    background_color: (47/255, 50/255, 57/255, 1) if self.state=='normal' else (150/255, 200/255, 255/255, .1)
    canvas:
        Color:
            rgba: 150/255, 200/255, 255/255, .3
        Line:
            width: 1
            rectangle: self.x + 1, self.y + 1, self.width - 1, self.height - 1
    Image:
        source: GV.DIR_ICONS + 'edit.png'
        allow_stretch: True
        size_hint: None, None
        size: 16, 16
        pos: self.parent.x + 4, self.parent.y + 4

<AddModuleBtn>:
    size: 24, 24
    size_hint: None, None
    background_down: ''
    background_normal: ''
    background_color: (47/255, 50/255, 57/255, 1) if self.state=='normal' else (150/255, 200/255, 255/255, .1)
    canvas:
        Color:
            rgba: 150/255, 200/255, 255/255, .3
        Line:
            width: 1
            rectangle: self.x + 1, self.y + 1, self.width - 1, self.height - 1
    Image:
        source: GV.DIR_ICONS + 'add.png'
        allow_stretch: True
        size_hint: None, None
        size: 16, 16
        pos: self.parent.x + 4, self.parent.y + 4

<DelModuleBtn>:
    size: 24, 24
    size_hint: None, None
    background_down: ''
    background_normal: ''
    background_color: (47/255, 50/255, 57/255, 1) if self.state=='normal' else (150/255, 200/255, 255/255, .1)
    canvas:
        Color:
            rgba: 150/255, 200/255, 255/255, .3
        Line:
            width: 1
            rectangle: self.x + 1, self.y + 1, self.width - 1, self.height - 1
    Image:
        source: GV.DIR_ICONS + 'del.png'
        allow_stretch: True
        size_hint: None, None
        size: 16, 16
        pos: self.parent.x + 4, self.parent.y + 4

# --------------------- Select Widget Style -------------------------------------------------------------------------- #
<ModuleSystemSpinnerOption>:
    color: GV.RGBA_TITLE_COL
    font_size: 18
    background_normal: ''
    background_color: GV.RGBA_BG_DARK
    canvas:
        Color:
            rgba: GV.RGBA_BG_LIGHT
        Line:
            width: 3
            rectangle: self.x + 1, self.y + 1, self.width - 1, self.height - 1

# ------------------- Select Widget ---------------------------------------------------------------------------------- #
<ModuleSystemSpinner>:
    size_hint: 1, 1
    pos_hint: {'x': 0, 'y': 0}
    text: 'System'
    font_size: 18
    background_normal: ''
    background_color: GV.RGBA_BG_DARK
    color: GV.RGBA_TITLE_COL
    on_text: self.selected_text(self.text)

# ----- Module Label ------------------------------------------------------------------------------------------------- #
<ModuleMapLabel>:
    size_hint: None, None
    size: self.texture_size
    font_size: 16
    color: GV.RGBA_WHITE50
    opacity: 0

# ----- Color Widget ------------------------------------------------------------------------------------------------- #
<ColorWidget>:
    size_hint: None, None
    size: 15, 15
    canvas:
        Color:
            rgb: self.color_bk
        Rectangle:
            size: self.size

# ----- Bit Label Object --------------------------------------------------------------------------------------------- #
<BitLabel>:
    size_hint: None, None
    size: self.texture_size

# ----- Add/Edit Label ----------------------------------------------------------------------------------------------- #
<ModuleEditLabel>:
    size_hint: None, None
    size: self.texture_size
    color: GV.RGBA_WHITE50

# ----- Add/Edit TextInput ------------------------------------------------------------------------------------------- #
<ModuleEditTextInput>:
    size_hint: None, None
    multiline: False
    font_size: 14
    size: 60, 22
    padding: 3, 3, 3, 3

<FunctionTextInput>:
    size_hint: None, None
    multiline: False
    font_size: 14
    size: 60, 22
    padding: 3, 3, 3, 3

<ColorTextInput>:
    size_hint: None, None
    multiline: False
    font_size: 14
    size: 60, 22
    padding: 3, 3, 3, 3

# ----- Add/Edit Spinner --------------------------------------------------------------------------------------------- #
<ModuleEditSpinner>:
    size_hint: None, None
    background_normal: ''
    background_color: GV.RGBA_BG_DARK

<ModuleEditSpinnerOption>:
    background_normal: ''
    background_color: GV.RGBA_BG_DARK
    canvas:
        Color:
            rgba: GV.RGBA_BG_LIGHT
        Line:
            width: 3
            rectangle: self.x + 1, self.y + 1, self.width - 1, self.height - 1

# ----- Add/Edit Spinner --------------------------------------------------------------------------------------------- #
<ModuleEditBitSpinner>:
    size_hint: None, None
    background_normal: ''
    background_color: GV.RGBA_BG_DARK
    size: 100, 20
    font_size: 10

<ModuleEditBitSpinnerOption>:
    background_normal: ''
    background_color: GV.RGBA_BG_DARK
    size: 100, 20
    font_size: 10
    canvas:
        Color:
            rgba: GV.RGBA_BG_LIGHT
        Line:
            width: 1
            rectangle: self.x + 1, self.y + 1, self.width - 1, self.height - 1

# ----- Cancel/Save Button Module interface -------------------------------------------------------------------------- #
<ModuleEditSaveBtn>:
    size_hint: None, None
    size: 90, 18
    background_normal: ''
    background_color: GV.RGBA_BG_DARK if self.state == 'normal' else GV.RGBA_BG_LIGHT
    disabled: True
    opacity: 0
    canvas:
        Color:
            rgba: GV.RGBA_SUCCESS
        Line:
            rectangle: self.x, self.y, self.width, self.height
            width: 1

<ModuleEditCancelBtn>:
    size_hint: None, None
    size: 90, 18
    background_normal: ''
    background_color: GV.RGBA_BG_DARK if self.state == 'normal' else GV.RGBA_BG_LIGHT
    canvas:
        Color:
            rgba: GV.RGBA_ERROR
        Line:
            rectangle: self.x, self.y, self.width, self.height
            width: 1

